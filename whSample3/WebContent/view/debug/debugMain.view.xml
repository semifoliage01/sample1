<!-- debugMain.view.xml -->
<!-- debugMain page -->
<mvc:View
	controllerName="whSample3.controller.employee.employeeDetail"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"
		   
	xmlns:l="sap.ui.layout"
	xmlns:core="sap.ui.core"
	xmlns:tnt="sap.tnt"
	xmlns:f="sap.ui.layout.form"
	
  	displayBlock="true">
  		<Page
					title="{i18n>title}"
					class="sapUiContentPadding">
					<content>
						<MessageStrip
							text="{i18n>warning}"
							type="Warning"
							showIcon="true"
							showCloseButton="false"
							class="sapUiMediumMarginBottom">
						</MessageStrip>

						<VBox>
							<Button
								id="myButton"
								icon="sap-icon://action"
								text="{i18n>doSomething}"
								press=".onPress"
								type="Emphasized"/>
							<Label
								id="LabelWithMissingI18NText"
								text="{i18n>Label_Missing_I18N_Text}"/>
						</VBox>
					</content>
				</Page>
  	
</mvc:View>